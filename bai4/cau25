  input_string = input("Nhập một chuỗi các số, tách nhau bởi dấu cách: ")

numbers list (map(int, input_string.split()))

odd_numbers = [num for num in numbers if num % 2 != 0]

print("Các số lẻ trong danh sách là:")

print (odd_numbers)
