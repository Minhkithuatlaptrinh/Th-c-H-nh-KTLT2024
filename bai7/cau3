def read_file(file_path):
    try:
        with open(file_path, 'r') as file:
            content = file.read()
    
        print(content)
    
    except FileNotFoundError:
        print(f"Không tìm thấy tệp: {file_path}")

file_path = input("Nhập đường dẫn tới tệp: ")

read_file(file_path)
