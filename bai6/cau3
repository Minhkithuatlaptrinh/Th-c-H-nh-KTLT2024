print("Sinh vien : Nguyen Hong Minh")
print("Ma so SV: 235752021610112")
class Nguoi(object):
    def getGender(self):
        return "Unknown"

class Nam(Nguoi):
    def getGender(self):
        return "Nam"

class Nu(Nguoi):
    def getGender(self):
        return "Nữ"

aNam = Nam()
aNu = Nu()

print(aNam.getGender())  # Kết quả sẽ là "Nam"
print(aNu.getGender())   # Kết quả sẽ là "Nữ"

  
