print("Sinh vien : Nguyen Hong Minh")
print("Ma so SV: 235752021610112")
class MyString:
    def __init__(self):
        self.string = ""
    
    def get_String(self):
        self.string = input("Nhập một chuỗi: ")
    
    def print_String(self):
        print(self.string.upper())


my_str = MyString()
my_str.get_String()
my_str.print_String()
